datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Book {
  id String @id @default(uuid())
  title String
  description String
  author String
  releaseYear Int
  barCode String @unique

  @@map("books")
}

model User {
  id String @id @default(uuid())
  email String @unique
  password String
  userName String @unique
  isAdmin Boolean

  @@map("users")
}